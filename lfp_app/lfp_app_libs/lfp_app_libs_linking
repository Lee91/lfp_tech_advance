#lib double list

ifeq ($(LFP_LIBS_DLIST),y)
libs_app_libs_src-y := $(CUR_PATH)/lfp_libs_dlist
libs_app_libs_obj-y := -llfp_libs_dlist
endif

#lib single list

ifeq ($(LFP_LIBS_SLIST),y)
libs_app_libs_src-y += $(CUR_PATH)/lfp_libs_slist
libs_app_libs_obj-y += -llfp_libs_slist
endif

#lib socket
ifeq ($(LFP_LIBS_SOCKET),y)
libs_app_libs_src-y += $(CUR_PATH)/lfp_libs_socket
libs_app_libs_obj-y += -llfp_libs_socket
endif

#lib threadpool
ifeq ($(LFP_LIBS_THREADPOOL),y)
libs_app_libs_src += $(CUR_PATH)/lfp_libs_threadpool
libs_app_libs_obj-y += -llfp_libs_threadpool
endif




